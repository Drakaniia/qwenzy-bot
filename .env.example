DISCORD_TOKEN=your_discord_bot_token
CLIENT_ID=your_discord_application_client_id
GUILD_ID=your_discord_server_id

# Lavalink (recommended for stable music playback)
# Local docker-compose defaults:
#   host: localhost (or "lavalink" if running the bot inside docker-compose)
#   port: 2333
#   password: youshallnotpass
LAVALINK_HOST=localhost
LAVALINK_PORT=2333
LAVALINK_PASSWORD=youshallnotpass
LAVALINK_SECURE=false
# Search prefix used by Lavalink (depends on enabled sources/plugins in Lavalink)
# Common values: ytsearch, ytmsearch, scsearch
LAVALINK_SEARCH_PREFIX=ytsearch

# Optional: YouTube cookie for play-dl (legacy path). Not needed when using Lavalink.
# YOUTUBE_COOKIE=your_youtube_session_cookie

# GEMINI_API_KEY=your_google_gemini_api_key  # Optional: only needed for AI features
